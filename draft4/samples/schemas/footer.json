{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://json-schema.org/footer#",
  "type": "object",
  "properties": {
    "searchBar": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "icon": {
          "type": "string"
        }
      },
      "required": [
        "label",
        "text",
        "icon"
      ]
    },
    "topLinks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "target": {
            "type": "string"
          }
        }
      }
    },
    "fcaLinksRail": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "image": {
            "type": "string"
          },
          "alt": {
            "type": "string"
          },
          "target": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        }
      }
    },
    "brandLinksRail": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "linkType": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "alt": {
            "type": "string"
          },
          "text": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "target": {
            "type": "string"
          }
        }
      }
    },
    "footerLinksRail": {
      "type": "object",
      "properties": {
        "columns": {
          "type": "string"
        },
        "externalLinks": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "layout": {
                "type": "string"
              },
              "heading": {
                "type": "string"
              },
              "links": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string"
                    },
                    "image": {
                      "type": "string"
                    },
                    "alt": {
                      "type": "string"
                    },
                    "icon": {
                      "type": "string"
                    },
                    "url": {
                      "type": "string"
                    },
                    "target": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "internalLinks": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "standardLinks": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "heading": {
                    "type": "string"
                  },
                  "link": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "text": {
                          "type": "string"
                        },
                        "url": {
                          "type": "string"
                        },
                        "target": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "copyright": {
      "type": "string"
    }
  },
  "required": [
    "searchBar",
    "topLinks",
    "fcaLinksRail",
    "brandLinksRail",
    "footerLinksRail",
    "copyright"
  ]
}
